{
  "name": "solid-fundamentals",
  "version": "1.0.0",
  "description": "GymPass style app.",
  "main": "index.js",
  "scripts": {
    "build": "tsup src --out-dir build",
    "start": "node build/server.js",
    "dev": "tsx watch src/server.ts",
    "test:create-prisma-env": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-env": "npm link vitest-environment-prisma",
    "pretest": "run-s test:create-prisma-env test:install-prisma-env",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --passWithNoTests --coverage run",
    "test:ui": "vitest --ui",
    "test:e2e": "npm run test -- e2e",
    "migrate": "prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/bcryptjs": "2.4.3",
    "@types/node": "20.5.7",
    "@types/supertest": "2.0.12",
    "@vitest/coverage-v8": "0.34.3",
    "eslint": "8.48.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.2.0",
    "supertest": "6.3.3",
    "tsup": "7.2.0",
    "tsx": "3.12.7",
    "typescript": "5.2.2",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@fastify/jwt": "7.2.1",
    "@prisma/client": "5.2.0",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "fastify": "4.22.1",
    "zod": "3.22.2"
  }
}
